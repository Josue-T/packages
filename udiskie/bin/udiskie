#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'udiskie==0.8.0','console_scripts','udiskie'
__requires__ = 'udiskie==0.8.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('udiskie==0.8.0', 'console_scripts', 'udiskie')()
    )
